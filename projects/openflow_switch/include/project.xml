<?xml version="1.0" encoding="UTF-8"?>
<nf:project xmlns:nf="http://www.NetFPGA.org/NF2_register_system" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.NetFPGA.org/NF2_register_system NF2_register_system.xsd ">
  <nf:name>OpenFlow Switch</nf:name>
  <nf:description>OpenFlow Switch</nf:description>
  <nf:version_major>1</nf:version_major>
  <nf:version_minor>0</nf:version_minor>
  <nf:version_revision>4</nf:version_revision>
  <nf:dev_id>0</nf:dev_id>
  <nf:use_modules>
    core/io_queues/cpu_dma_queue
    core/io_queues/ethernet_queue
    contrib/ucsd/gig_eth_mac
    core/input_arbiter/rr_input_arbiter
    core/nf2/generic_top
    core/nf2/reference_core
    core/user_data_path/reference_user_data_path
    core/user_data_path/udp_reg_master
    core/output_queues/bram_output_queues
    core/io/mdio
    core/cpci_bus
    core/dma
    core/io_queues/add_rm_hdr
    core/utils/generic_regs
    core/utils
  </nf:use_modules>
  <nf:memalloc layout="reference">
    <nf:group name="core1">
      <nf:instance name="device_id" />
      <nf:instance name="dma" base="0x0500000"/>
      <nf:instance name="mdio" base="0x0440000"/>
      <nf:instance name="nf2_mac_grp" count="4" />
      <nf:instance name="cpu_dma_queue" count="4" />
    </nf:group>
    <nf:group name="udp">
      <nf:instance name="in_arb" base="0x02000100"/>
      <nf:instance name="vlan_remover" />
      <nf:instance name="header_parser" />
      <nf:instance name="top_ddos" />
      <nf:instance name="exact_match" />
      <nf:instance name="watchdog" base="0x02001400"/>
      <nf:instance name="match_arbiter" />
      <nf:instance name="openflow_output_port_lookup" base="0x02000000"/>
      <nf:instance name="opl_processor" />
      <nf:instance name="wildcard_match" base="0x02001000"/>
      <nf:instance name="bram_output_queues" base="0x02000300"/>
    </nf:group>
  </nf:memalloc>
</nf:project>
